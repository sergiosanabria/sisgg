{%if form %}{% form_theme form "SisGGFinalBundle:Form:personalizarCampos.html.twig" %}{%endif%}
{%if r==1 %}
<form class="form-horizontal" id="etiquetaForm" onsubmit="modificarEtiqueta(event);">
    {%if error %}
     <div class="alert alert-block">
                <button type="button" class="close" data-dismiss="alert">×</button>
                <h4>¡Cuidado!</h4>
                {{error}}
            </div>
        {%endif%}
<fieldset id="datosEtiqueta">

    {{form_widget(form)}}
   
    {{form_rest(form)}}

        <div id="divCuenta" style="display: none"> <input type="hidden" id="idEtiqueta" name="idEtiqueta"> </div>
        <div class="form-actions">
            <button class="btn btn-primary"  type="submit">Modificar</button>
            <button class="btn" data-dismiss="modal" aria-hidden="true">Cancelar</button>
        </div>
         
</fieldset>
</form>
{%elseif r==-1%}
        <div class="alert alert-block">
                <button type="button" class="close" data-dismiss="alert">×</button>
                <h4>¡Cuidado!</h4>
                 La etiqueta no existe o no se encuentra activo.
            </div>
{%elseif r==0%}
<div class="alert alert-success">
    <button type="button" class="close" data-dismiss="alert">×</button>
    <h4>¡Muy bien!</h4>
     La etiqueta se ha registrado correctamente.
</div>
{%elseif r==2 %}

    {%if error is null %}
<div class="alert alert-success">
    <button type="button" class="close" data-dismiss="alert">×</button>
    La etiqueta se ha eliminado correctamente.
    {%else%}
    <div class="alert alert-danger">
    <button type="button" class="close" data-dismiss="alert">×</button>
    {{error}}
    {%endif%}
</div>
{%endif%}